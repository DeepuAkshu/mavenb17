pipeline {
    agent any
    def mavenHome  = tool name: 'maven3.9.1'
    stages {
        stage('SCM') {
            steps {
                git 'https://github.com/learnaws288/mavenb17.git'
            }         
}
       
        stage('Build by Maven Package') {
            steps {
                sh "${mavenHome}/bin/mvn clean package"
            }
        }
    }
}
